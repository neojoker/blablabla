/*jQuery elements*/
.ui-widget {
	font-size: 1em;
}

/*Selectable Elements*/
#feedback { font-size: 1.4em; }
#selectable .ui-selecting { background: #52B4E6; }
#selectable .ui-selected { background: #52B4E6; color: white; }
#selectable { list-style-type: none; margin: 0; padding: 0; width: 60%; }
#selectable li { margin: 3px; padding: 0.4em; font-size: 1.0em; height: 14px; }

/*Sortable Elements*/
#sortable { list-style-type: none; margin: 0; padding: 0; width: 60%; }
#sortable li { margin: 0 3px 3px 3px; padding: 0.4em; padding-left: 1.5em; font-size: 1.0em; height: 14px; }
#sortable li span { position: absolute; margin-left: -1.3em; }

/*Buttons*/
#format { margin-top: 2em; }

/*Modal Form*/
#dialog-form label, input { display:block; }
#dialog-form input.text { margin-bottom:12px; width:55%; padding: .4em; }
#dialog-form fieldset { padding:0; border:0; margin-top:25px; }
#dialog-form h1 { font-size: 1.2em; margin: .6em 0; }
div#users-contain { width: 350px; margin: 20px 0; }
div#users-contain table { margin: 1em 0; border-collapse: collapse; width: 100%; }
div#users-contain table td, div#users-contain table th { border: 1px solid #eee; padding: .6em 10px; text-align: left; }
.ui-dialog .ui-state-error { padding: .3em; }
#dialog-form  .validateTips { border: 1px solid transparent; padding: 0.3em; }


.ir { display: block; text-indent: -999em; overflow: hidden; background-repeat: no-repeat; text-align: left; direction: ltr; }
.hidden { display: none; visibility: hidden; }
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }
.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus { clip: auto; height: auto; margin: 0; overflow: visible; position: static; width: auto; }
.invisible { visibility: hidden; }
.clearfix:before, .clearfix:after { content: "\0020"; display: block; height: 0; overflow: hidden; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }


