/*Header*/

header {
	height: 115px;
	position: relative;
}

header .logo {
	position: absolute;
	left: 20px;
	top: 14px;
}

header h1 {
	position: absolute;
	left: 130px;
	top: 28px;
	font-size: 1.5em;
	text-transform: uppercase;
}

header nav.main {
	position: absolute;
	left: 475px;
	top: 25px;	
}

header nav.main ul li {
	float: left;
	margin: 0 7px 0 0;
	border-right: 1px solid #999;
	padding: 0 7px 0 0;
}

header nav.main ul li a{
	color: #999;
	text-decoration: none;
	font-size: 0.9em;
}

header nav.main ul li a:hover{
	color: #4d4d4d;
}

header nav.sub {
	position: absolute;
	right: 0;
	bottom: 8px;	
}


header nav.sub a{
	color: #999;
	text-decoration: none;
	margin: 0 7px 0 0;
	border-right: 1px solid #999;
	padding: 0 7px 0 0;
	font-size: 0.9em;
}

#container header nav .last {
	border: none;
}

header nav.sub a:hover{
	color: #4d4d4d;
}

/*Content*/

body {
	background-color: #cbcbcb;
}

#container {
	width: 900px;
	margin: 0 auto;
	background-color: #f7f7f7;
	overflow: hidden;
}

#container p {
	margin: 0 0 10px 0;
}

.rows .row {
	padding: 10px;
	overflow: hidden;
}

.rows .row h2 {
	margin: 0 0 10px 0;
}

.rows .row a {
	text-decoration: none;
	font-size: 0.9em;
	color: #666;
	display: block;
	float: left;
	width: 75px;
	text-align: center;
	margin: 0 5px;
}

.rows .row a:hover {
	color: #4d4d4d;
}

.rows .row a span{
	display: block;
}

.rows .odd {
	background-color: #e8e8e8;
}

.rows .even {
	background-color: #efefef;
}

/* Notification */
#notification {
	border-top: 1px dotted #666;
	border-bottom: 1px dotted #666;
	padding: 6px 15px;
	margin: 3px 0 0;
}


/*Styling to separate ui items*/

.item {
	border: 1px solid #eee;
	padding: 20px;
	background: #fff;
}

.item h2 {
	margin: 10px 0 10px 0;
}



/*  Dropdown Menu  */
/* this is the only class selector you need. */ 
.drop{
	max-width:300px;
	display: none;
}

	.drop p{
		font:12px/16px Verdana, Sans-Serif;
		color:#fff;
	}
	
	.drop a{
		color:#999;
		text-decoration:none;
	}
	
	.drop a:hover{
		color:#4d4d4d;
	}

	.drop ul{
		list-style-type:none;
		color:#ee0000;
		margin:0px 0px;
		padding-left:0;
		float:left;
		border-left:1px dashed #666;
	}
	
	.drop ul:first-child{
		border:0px;
		padding:0px 18px;
	}

		.drop ul li{
			margin:5px 0px 0px;
			font-size: 0.9em;
		}
		
		.drop ul li:first-child{
			margin:0px;
		}
		
	.drop strong{
		color:#ee0000;
		font-weight:bold;
	}
	
	.drop input[type=text], input[type=email]{
		padding:0px;
		margin:0px;
		font-size:11px;
		color:#ccc;
		border:0px;
		background:none;
		border-bottom:1px dashed #fff;
	}
	
	.drop input[type=submit]{
		padding:5px 10px;
		background:#fff;
		background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#ccc));
		background: -moz-linear-gradient(top,  #fff,  #ccc);
		border:0px;
		border-radius:5px;
		-webkit-border-radius:5px;
		-moz-border-radius:5px;
		-webkit-box-shadow:0px 0px 10px rgba(66,66,66, .25);
		-moz-box-shadow:0px 0px 10px rgba(66,66,66, .25);
		float:right;
	}
	
	.drop textarea{
		font-size:11px;
		color:#ccc;
		margin:10px 0px;
		padding:5px;
		width:288px;
		height:50px;
		background:none;
		border:1px dashed #fff;

	}


